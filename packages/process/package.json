{
	"name": "@pkg/process",
	"version": "1.0.0",
	"description": "BPMN SDK wrapper for DSL->IR->engine, start/signal/message, human task/SLA",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "biome check .",
		"format": "biome format --write .",
		"test": "vitest run",
		"clean": "rm -rf dist"
	},
	"keywords": ["bpmn", "process", "workflow"],
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@types/node": "^20.14.0",
		"tsup": "^8.0.0",
		"typescript": "^5.5.0",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"bpmn-js": "^17.8.0",
		"@effect/schema": "^0.67.16",
		"effect": "^3.10.4"
	}
}
