{
	"name": "@gftdcojp/performer-data",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"version": "1.0.0",
	"description": "effect-cypher + neogma adapter for type-safe Cypher/Tx utilities/constraints",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "biome check .",
		"format": "biome format --write .",
		"test": "vitest run",
		"clean": "rm -rf dist"
	},
	"keywords": ["data", "neo4j", "cypher", "neogma"],
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@types/node": "^20.14.0",
		"tsup": "^8.0.0",
		"typescript": "^5.5.0",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"effect": "^3.6.0",
		"neogma": "^1.15.1",
		"neo4j-driver": "^5.22.0"
	}
}
