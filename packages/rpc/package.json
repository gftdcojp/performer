{
	"name": "@gftdcojp/performer-rpc",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"version": "1.0.0",
	"description": "Thin RPC layer for procedure registration and dispatch - framework agnostic",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "biome check .",
		"format": "biome format --write .",
		"test": "vitest run",
		"clean": "rm -rf dist"
	},
	"keywords": ["rpc", "procedure", "dispatch"],
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@types/node": "^20.14.0",
		"tsup": "^8.0.0",
		"typescript": "^5.5.0",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"@gftdcojp/performer-process": "workspace:*",
		"@gftdcojp/performer-error-handling": "workspace:*"
	}
}
